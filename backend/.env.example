# DeepSeek AI Configuration
DEEPSEEK_TOKEN=your-deepseek-token-here

# JSearch API Configuration (RapidAPI)
JSEARCH_API_KEY=your-rapidapi-key-here
JSEARCH_API_HOST=jsearch.p.rapidapi.com
JSEARCH_URL=https://jsearch.p.rapidapi.com/search

# SerpAPI Configuration
SERPAPI_KEY=your-serpapi-key-here
SERPAPI_HOST=serpapi.com
SERPAPI_API_URL=https://serpapi.com/search

# Google Cloud Configuration
GCS_BUCKET=jobs_ingestion_data_bucket
GCS_PREFIX=staging/jsearch
BQ_PROJECT_ID=job-recommendations-app
REGION=europe-north1
BQ_DATASET=jobs_ds
BQ_TABLE=jobs_jsearch_raw
EVENT_BQ_TABLE=events_google_raw

# Development Configuration
DRY_RUN=true

# Add your actual DeepSeek token to .env file (not committed to Git)
# Get your token from: https://models.github.ai/
# Get your RapidAPI key from: https://rapidapi.com/
# Get your SerpAPI key from: https://serpapi.com/